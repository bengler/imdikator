{
  "name": "imdikator",
  "version": "0.0.0",
  "private": "true",
  "description": "Imdikator",
  "main": "index.js",
  "author": "Bengler AS",
  "scripts": {
    "phantom": "mochify --require ./.init.js --extension=.jsx --recursive --transform babelify",
    "watch-test": "npm run phantom -- --watch",
    "cover": "npm run phantom -- --plugin [ mochify-istanbul --instrumenter babel-istanbul --report text ]",
    "test": "npm run cover",
    "start": "remon --require ./.init.js ./bin/server.js",
    "bloat": "staticr --stdout --require ./.init.js --route /js/bundles/main.js static-routes/browserify-bundles.js | discify --open",
    "lint": "eslint --quiet --ext .jsx,.js .",
    "compile-assets": "staticr --require babel/register public/build ./static-routes ",
    "import-regions": "DEBUG=imdikator* babel-node ./bin/importRegions",
    "import-groups": "DEBUG=imdikator* babel-node ./bin/importGroups",
    "import-tables": "DEBUG=imdikator* babel-node ./bin/importTables",
    "import-dimensions": "DEBUG=imdikator* babel-node ./bin/importDimensions",
    "find-unused-tables": "DEBUG=imdikator* babel-node ./bin/findUnusedTables",
    "pre-commit-check": "npm test && (npm run lint -- --quiet || (echo 'Warning: project has lint errors. Please fix and re-commit with `git commit --amend`' && echo ''))"
  },
  "pre-commit": [
    "pre-commit-check"
  ],
  "dependencies": {
    "bluebird": "^2.9.34",
    "d3": "^3.5.6",
    "debug": "^2.2.0",
    "duplexify": "^3.4.2",
    "http-hash": "^2.0.0",
    "lodash": "^3.10.1",
    "once": "^1.3.2",
    "qs": "^4.0.0",
    "react": "^0.13.3",
    "react-redux": "^1.0.1",
    "redux": "^1.0.1"
  },
  "devDependencies": {
    "@bengler/imdi-dataset": "^4.1.6",
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.10",
    "babel-istanbul": "^0.3.17",
    "babelify": "^6.2.0",
    "browserify": "^11.0.1",
    "bundle-collapser": "^1.2.1",
    "chai": "^3.2.0",
    "csv-parse": "^1.0.0",
    "defaults": "^1.0.2",
    "disc": "^1.3.2",
    "envify": "^3.4.0",
    "error-capture-middleware": "0.0.2",
    "eslint": "^1.2.1",
    "eslint-config-bengler": "^0.2.10",
    "eslint-plugin-import": "^0.7.8",
    "eslint-plugin-react": "^3.2.3",
    "express": "^4.13.3",
    "imdi-styles": "git+ssh://git@github.com/netliferesearch/imdi-styles.git",
    "less": "^2.5.1",
    "mocha": "^2.2.5",
    "mochify": "^2.13.0",
    "mochify-istanbul": "^2.3.0",
    "phantomjs": "^1.9.18",
    "pre-commit": "^1.1.1",
    "quickreload": "^1.0.8",
    "rebundler": "^0.2.0",
    "remon": "^1.0.1",
    "rx": "^3.1.1",
    "rx-node": "^1.0.1",
    "spawn-stream": "^1.0.2",
    "staticr": "^2.1.3",
    "uglify-js": "^2.4.24"
  }
}
