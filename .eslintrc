{
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "eslint-plugin-import"
  ],
  "extends": [
    "bengler",
    "bengler/react"
  ],
  "rules": {
    "import/no-unresolved": 2,
    "import/named": 1,
    "import/default": 2,
    "import/namespace": 2,
    "import/no-require": 2,
    "import/no-named-as-default": 2,
    "import/export": 2
  },
  "settings": {
    "import/resolve": {
      "extensions": [".js", ".jsx", ".json"]
    }
  }
}
