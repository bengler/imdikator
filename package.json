{
  "name": "imdikator",
  "version": "0.0.0",
  "private": "true",
  "description": "Imdikator",
  "main": "index.js",
  "author": "Bengler AS",
  "scripts": {
    "test": "mocha --require ./.init.js test/**/*",
    "start": "remon --require ./.init.js ./bin/server.js",
    "bloat": "staticr --stdout --require ./.init.js --route /bundles/default.js static-routes/browserify-bundles.js | discify --open",
    "lint": "eslint --quiet --ext .jsx,.js .",
    "compile-assets": "staticr --require babel/register public ./static-routes ",
    "import-regions": "DEBUG=imdikator* babel-node ./bin/importRegions",
    "import-groups": "DEBUG=imdikator* babel-node ./bin/importGroups",
    "import-tables": "DEBUG=imdikator* babel-node ./bin/importTables",
    "import-dimensions": "DEBUG=imdikator* babel-node ./bin/importDimensions",
    "find-unused-tables": "DEBUG=imdikator* babel-node ./bin/findUnusedTables"
  },
  "pre-commit": [
    "pre-commit-check"
  ],
  "dependencies": {
    "babel": "^5.4.7",
    "babelify": "^6.1.2",
    "bluebird": "^2.9.14",
    "body-parser": "^1.12.2",
    "browserify": "^10.2.1",
    "bundle-collapser": "^1.1.4",
    "classnames": "^1.2.0",
    "d3": "^3.5.0",
    "defaults": "^1.0.2",
    "dom-event": "0.0.4",
    "domready": "~1.0.7",
    "dotty": "0.0.2",
    "duplexify": "^3.3.0",
    "envify": "^3.4.0",
    "express": "^4.12.3",
    "helmet": "^0.7.0",
    "imdi-no": "git+ssh://git@github.com/bengler/imdi-no.git",
    "node-sass": "^3.1.2",
    "once": "^1.3.1",
    "qs": "^2.4.1",
    "react": "^0.12.2",
    "react-a11y": "0.0.6",
    "react-menu": "0.0.6",
    "react-scroll": "^0.18.0",
    "reactify": "^1.1.1",
    "rebundler": "^0.0.4",
    "reflux": "^0.2.7",
    "sigmund": "^1.0.0",
    "spawn-stream": "^1.0.2",
    "staticr": "^2.1.2",
    "uglify-js": "^2.4.17",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.9",
    "chai": "^2.1.2",
    "csv-parse": "^0.1.0",
    "debug": "^2.1.3",
    "disc": "^1.3.2",
    "error-capture-middleware": "0.0.2",
    "eslint": "^1.1.0",
    "eslint-config-bengler": "^0.2.10",
    "eslint-plugin-import": "^0.7.8",
    "eslint-plugin-react": "^3.2.3",
    "lodash": "^3.5.0",
    "mocha": "^2.2.1",
    "pre-commit": "^1.0.7",
    "quickreload": "^1.0.6",
    "reactify": "^1.1.0",
    "remon": "^1.0.1",
    "rx": "^2.5.2",
    "rx-node": "^1.0.0"
  }
}
